{
    "comments": {
        "blockComment": [
            "/*",
            "*/"
        ],
        "lineComment": "//"
    },
    "brackets": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"],
    ],
    "autoClosingPairs": [
        { "open": "(", "close": ")", "notIn": ["string"]},
        { "open": "[", "close": "]", "notIn": ["string"]},
        { "open": "{", "close": "}", "notIn": ["string"]},
        { "open": "\"", "close": "\"", "notIn": ["string"]},
        { "open": "'", "close": "'", "notIn": ["string", "comment"]},
    ],
    "autoCloseBefore": ";.,=}])> \n\t",
    "surroundingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["{", "}"],
        ["'", "'"],
        ["\"", "\""],
    ],
}